        float3 xformOp:translate = (-0.04417, 0.02906, 0.08711)
        uniform token[] xformOpOrder = ["xformOp:translate", "xformOp:orient", "xformOp:scale"]
    }

    def "Hydrogen_2" (
        active = true
        references = @Hydrogen.usda@
    )
    {
