        uniform token[] xformOpOrder = ["xformOp:translate", "xformOp:orient", "xformOp:scale"]

        over "Sphere"
        {
            over "DefaultMaterial"
            {
                token outputs:surface.connect = </Root/Hydrogen_3/Sphere/DefaultMaterial/DefaultSurfaceShader.outputs:surface>
            }
        }
    }
